                                    span.txt_thanks Thank you <br> for your response!
                                  .txt_center.d-block.mb-5
                                    hr.hr_site
                                  .paragraf.txt_center.my-5
                                    .txt_size--em3 We hope you can join us <br> in future gatherings
                                  .row
                                    .col.mb-5
                                      button.btn.btn_submit.w-auto.closeSection EXIT
                    .footer.bgc--gains.fixed--bottom.txt_center
