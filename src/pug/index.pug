                                    .w-75.mx-auto
                                        .title.txt_upper.mb-5.txt_center
                                            .txt_upper.txt_bold.cl--black.txt_size--em3 RVSP Form
                                        form
                                            .row
                                                .col
                                                    .form-group
                                                        label.txt_bold
                                                            | First name
                                                        input.form-control.firstName#firstName(type="text", placeholder="First name", name="firstName")
                                                        .alert.hidden
                                                            small Add first names
                                                .col
                                                    .form-group
                                                        label.txt_bold
                                                            | Last name
                                                        input.form-control.lastName#lastName(type="text", placeholder="Last name", name="lastName")
                                                        .alert.hidden
                                                            p Add last names
                                            .row
                                                .col.mb-5
                                                    .form-group
                                                        label.txt_bold
                                                            | Email
                                                        input.form-control.email#email(type="text", placeholder="Email", name="email")
                                                        .alert.hidden
                                                            p Add your email
                                            .row
                                                .col.mb-5
                                                    button.btn.btn_submit#findMyInvitation Find my Invitation
                                #sectionECO.bdb--gains.bdb--x2.w-100.mb-5
